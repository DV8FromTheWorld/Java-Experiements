package dv8.output;

public class DebugOutput {
	private static boolean enabled = false;
	public static void init(){
		if(Main.runArgs[0] != null  && Main.runArgs[0].equals(1)){
			enabled = true;
			out("Debug Enabled");
		}
	}
	public static void out(String s){
		if(enabled){
			System.out.println(s);
		}
	}
}
