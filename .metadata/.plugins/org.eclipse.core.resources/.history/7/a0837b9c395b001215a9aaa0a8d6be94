package parse;

import java.io.*;
public class Main {
	public static void main(String[] args){
		
		try{
			String s = Main.class.getProtectionDomain().getCodeSource().getLocation().getPath();
			BufferedReader br = new BufferedReader(new FileReader(s +"yo.txt"));
			
			br.close();
		}catch(IOException e){
			e.printStackTrace();
		}
				
	}
}
